  "template": Object {
    "_beagleComponent_": "custom:note",
    "labels": "@{item.labels}",
    "noteId": "@{item.id}",
    "onRemove": Array [
      Object {
        "_beagleAction_": "beagle:setContext",
        "contextId": "data",
        "path": "notes",
        "value": "@{remove(data.notes, item)}",
      },
      Object {
        "_beagleAction_": "beagle:sendRequest",
        "method": "DELETE",
        "onError": Array [
          Object {
            "_beagleAction_": "custom:feedback",
            "text": "Connection error. Couldn't remove the label.",
            "type": "error",
          },
          Object {
            "_beagleAction_": "beagle:setContext",
            "contextId": "data",
            "path": "notes",
            "value": "@{insert(data.notes, item, index)}",
          },
        ],
        "onSuccess": Array [
          Object {
            "_beagleAction_": "custom:feedback",
            "text": "Note removed successfully!",
            "type": "success",
          },
        ],
        "url": "https://keep.beagle.io/note/@{item.id}",
      },
    ],
    "onSelect": Array [
      Object {
        "_beagleAction_": "beagle:setContext",
        "contextId": "global",
        "path": "selectedNote",
        "value": "@{item.id}",
      },
      Object {
        "_beagleAction_": "beagle:pushView",
        "route": Object {
          "url": "/details",
        },
      },
    ],
    "style": Object {
      "margin": Object {
        "all": Object {
          "type": "REAL",
          "value": 10,
        },
      },
    },
    "text": "@{item.text}",
    "title": "@{item.title}",
  },
}
`;

